
apt install apache2 libapache2-mod-php7.3 php php-common php-fpm php-mysql php7.3 php7.3-cli php7.3-common  php7.3-fpm php7.3-json php7.3-mysql php7.3-opcache php7.3-readline

systemctl stop apache2

apt install nginx

rm /etc/nginx/sites-enabled/default

wget https://dev.mysql.com/get/mysql-apt-config_0.8.15-1_all.deb && dpkg -i mysql-apt-config_0.8.15-1_all.deb && apt update

apt install mysql-server -y

apt install bird -y

systemctl disable bird

systemctl disable bird6

apt install zabbix-agent -y

systemctl enable zabbix-agent